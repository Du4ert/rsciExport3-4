<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filterConfig SYSTEM "../../../../lib/pkp/dtd/filterConfig.dtd">

<!--
  * filterConfig.xml
  *
  * Filter Configuration.
  -->
<filterConfig>
	<filterGroups>
		<!-- RSCI XML article output -->
		<filterGroup
			symbolic="issue=>rsci-xml"
			displayName="plugins.importexport.rsciexport.displayName"
			description="plugins.importexport.rsciexport.description"
			inputType="class::classes.issue.Issue"
			outputType="xml::dtd" />
	</filterGroups>
	<filters>
		<!-- RSCI article output -->
		<filter
			inGroup="issue=>rsci-xml"
			class="APP\plugins\importexport\rsciexport\filter\ArticleRSCIXmlFilter"
			isTemplate="0" />
	</filters>
</filterConfig>
